{
    "name": "cosapp_lab",
    "version": "0.16.0",
    "description": "Toolbox for managing and deploying CoSApp powered dashboards.",
    "keywords": [
        "jupyter",
        "jupyterlab",
        "jupyterlab-extension",
        "widgets"
    ],
    "homepage": "https://gitlab.com/cosapp/cosapp_lab",
    "bugs": {
        "url": "https://gitlab.com/cosapp/cosapp_lab/issues"
    },
    "license": "Proprietary License",
    "author": {
        "name": "CoSApp Team",
        "email": "etienne.lac@safrangroup.com"
    },
    "files": [
        "lib/**/*.{d.ts,eot,gif,html,jpg,js,js.map,json,png,svg,woff2,ttf}",
        "style/**/*.{css,js,eot,gif,html,jpg,json,png,svg,woff2,ttf}",
        "src/**/*.{ts,tsx}"
    ],
    "main": "lib/index.js",
    "types": "lib/index.d.ts",
    "style": "style/index.css",
    "repository": {
        "type": "git",
        "url": "https://gitlab.com/cosapp/cosapp_lab.git"
    },
    "scripts": {
        "build": "tsc  && webpack --env MODE=prod && jlpm run build:labextension",
        "build:labextension": "jupyter labextension build .",
        "build:labextension:dev": "jupyter labextension build --development True .",
        "build:app": "tsc  && webpack --env TARGET=app",
        "build:lab": "tsc  && webpack --env TARGET=lab && jlpm build:labextension:dev",
        "build:all": "tsc  && webpack && jlpm build:labextension:dev",
        "build:webpack": "webpack --env TARGET=lab && jlpm build:labextension:dev",
        "build:docs": "sphinx-build -b html -d ./docs/_build/doctrees ./docs ./docs/_build",
        "clean:all": "rimraf buildjs/* && rimraf lib/* && rimraf cosapp_lab/lab_static/*  && rimraf cosapp_lab/app_static/static/dist/* &&  copyfiles ./package.json ./buildjs",
        "clean:lib": "rimraf buildjs/* && rimraf lib/* && rimraf cosapp_lab/lab_static/*",
        "clean:labextension": "rimraf cosapp_lab/lab_static/static/lib_index_* && rimraf cosapp_lab/lab_static/static/remoteEntry*",
        "eslint": "eslint . --ext .ts,.tsx --fix",
        "eslint:check": "eslint . --ext .ts,.tsx",
        "install:extension": "jupyter labextension develop --overwrite .",
        "copy:extension": "python ./cosapp_lab/utils/copy_extension.py",
        "watch": "run-p watch:src watch:labextension",
        "watch:src": "tsc -w",
        "watch:lab": "jupyter labextension watch .",
        "test": "jest src --silent "
    },
    "dependencies": {
        "@antv/g6": "^3.5.7",
        "@emotion/core": "^10.0.27",
        "@emotion/styled": "^10.0.27",
        "@jupyter-widgets/base": "^6.0.0",
        "@jupyter-widgets/controls": "^5.0.0",
        "@jupyter-widgets/html-manager": "^1.0.0",
        "@jupyterlab/application": "^4.0.0",
        "@jupyterlab/apputils": "^4.0.0",
        "@jupyterlab/coreutils": "^6.0.0",
        "@jupyterlab/notebook": "^4.0.0",
        "@jupyterlab/services": "^7.0.0",
        "@lumino/widgets": "^2.0.0",
        "@material-ui/core": "^4.9.3",
        "@material-ui/icons": "^4.5.1",
        "@material-ui/lab": "^4.0.0-alpha.33",
        "@projectstorm/react-diagrams": "^6.2.0",
        "closest": "^0.0.1",
        "codemirror": "^5.59.4",
        "dagre": "^0.8.4",
        "flexlayout-react": "^0.5.4",
        "global": "^4.4.0",
        "ify-loader": "^1.1.0",
        "lodash": "^4.17.15",
        "marked": "^1.2.9",
        "material-table": "1.62.0",
        "mathjs": "^6.2.5",
        "ml-matrix": "^6.5.0",
        "pathfinding": "^0.4.18",
        "paths-js": "^0.4.10",
        "plotly.js": "^1.57.1",
        "react-codemirror2": "^7.2.1",
        "react-dnd": "^16.0.0",
        "react-dnd-html5-backend": "^16.0.0",
        "react-draggable": "^4.4.3",
        "react-plotly.js": "^2.5.0",
        "react-redux": "^9.0.0",
        "react-select": "^5.0.0",
        "react-sortable-tree": "2.7.1",
        "react-table": "^7.6.0",
        "redux": "^5.0.0",
        "redux-thunk": "^3.0.0",
        "resize-observer-polyfill": "^1.5.1",
        "three": "^0.117.0",
        "vis": "^4.21.0-EOL"
    },
    "resolutions": {
        "lodash": "4.17.15",
        "react-sortable-tree": "2.7.1"
    },
    "devDependencies": {
        "@babel/core": "7",
        "@babel/preset-env": "7",
        "@cfaester/enzyme-adapter-react-18": "^0.8.0",
        "@jupyterlab/builder": "^4.0.0",
        "@testing-library/react": "^9.4.1",
        "@types/enzyme": "^3.10.5",
        "@types/jest": "24",
        "@types/json-schema": "^7.0.11",
        "@types/node": "^10.11.6",
        "@types/plotly.js": "^2.29.4",
        "@types/react": "^18.0.26",
        "@types/react-addons-linked-state-mixin": "^0.14.22",
        "@types/react-dom": "16.9.4",
        "@types/react-plotly.js": "^2.6.3",
        "@types/react-redux": "^7.1.5",
        "@types/react-sortable-tree": "^0.3.6",
        "@typescript-eslint/eslint-plugin": "^6.1.0",
        "@typescript-eslint/parser": "^6.1.0",
        "copyfiles": "^2.4.0",
        "css-loader": "^6.7.1",
        "enzyme": "^3.11.0",
        "eslint": "^8.36.0",
        "eslint-config-prettier": "^8.8.0",
        "eslint-plugin-prettier": "^5.0.0",
        "fs-extra": "^7.0.0",
        "jest": "24",
        "jest-canvas-mock": "^2.3.1",
        "mkdirp": "^0.5.1",
        "npm-run-all": "^4.1.5",
        "prettier": "^3.0.0",
        "prettier-eslint": "^12.0.0",
        "re-resizable": "^6.3.2",
        "redux-mock-store": "^1.5.4",
        "rimraf": "^5.0.1",
        "source-map-loader": "^1.0.2",
        "style-loader": "^3.3.1",
        "stylelint": "^15.10.1",
        "stylelint-config-recommended": "^13.0.0",
        "stylelint-config-standard": "^34.0.0",
        "stylelint-csstree-validator": "^3.0.0",
        "stylelint-prettier": "^4.0.0",
        "ts-jest": "24",
        "ts-loader": "^5.2.1",
        "typescript": "~5.0.2",
        "webpack-cli": "^4.5.0",
        "yjs": "^13.5.0"
    },
    "sideEffects": [
        "style/*.css",
        "style/index.js"
    ],
    "styleModule": "style/index.js",
    "publishConfig": {
        "access": "public"
    },
    "jupyterlab": {
        "extension": true,
        "outputDir": "cosapp_lab/lab_static",
        "sharedPackages": {
            "@jupyter-widgets/base": {
                "bundled": false,
                "singleton": true
            },
            "react": {
                "bundled": false,
                "singleton": true
            },
            "codemirror": {
                "bundled": false,
                "singleton": true
            }
        },
        "discovery": {
            "server": {
                "base": {
                    "name": "cosapp_lab"
                },
                "managers": [
                    "conda"
                ]
            }
        }
    },
    "eslintIgnore": [
        "node_modules",
        "dist",
        "coverage",
        "**/*.d.ts"
    ],
    "eslintConfig": {
        "extends": [
            "eslint:recommended",
            "plugin:@typescript-eslint/eslint-recommended",
            "plugin:@typescript-eslint/recommended",
            "plugin:prettier/recommended"
        ],
        "parser": "@typescript-eslint/parser",
        "parserOptions": {
            "project": "tsconfig.json",
            "sourceType": "module"
        },
        "plugins": [
            "@typescript-eslint"
        ],
        "rules": {
            "@typescript-eslint/no-unused-vars": [
                "warn",
                {
                    "args": "none"
                }
            ],
            "@typescript-eslint/no-explicit-any": "off",
            "@typescript-eslint/no-namespace": "off",
            "@typescript-eslint/no-use-before-define": "off",
            "@typescript-eslint/quotes": [
                "error",
                "single",
                {
                    "avoidEscape": true,
                    "allowTemplateLiterals": false
                }
            ],
            "curly": [
                "error",
                "all"
            ],
            "eqeqeq": "error",
            "prefer-arrow-callback": "error"
        }
    },
    "prettier": {
        "singleQuote": true,
        "trailingComma": "none",
        "arrowParens": "avoid",
        "endOfLine": "auto",
        "overrides": [
            {
                "files": "package.json",
                "options": {
                    "tabWidth": 4
                }
            }
        ]
    },
    "stylelint": {
        "extends": [
            "stylelint-config-recommended",
            "stylelint-config-standard",
            "stylelint-prettier/recommended"
        ],
        "plugins": [
            "stylelint-csstree-validator"
        ],
        "rules": {
            "csstree/validator": true,
            "property-no-vendor-prefix": null,
            "selector-class-pattern": "^([a-z][A-z\\d]*)(-[A-z\\d]+)*$",
            "selector-no-vendor-prefix": null,
            "value-no-vendor-prefix": null
        }
    }
}
